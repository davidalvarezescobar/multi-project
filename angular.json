{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "weather": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true,
          "changeDetection": "OnPush"
        },
        "@schematics/angular:service": {
          "skipTests": true
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/weather",
      "sourceRoot": "projects/weather/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/weather",
            "index": "projects/weather/src/index.html",
            "main": "projects/weather/src/main.ts",
            "polyfills": "projects/weather/src/polyfills.ts",
            "tsConfig": "projects/weather/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/weather/src/favicon.ico",
              "projects/weather/src/assets"
            ],
            "styles": [
              "./node_modules/@picocss/pico/css/pico.classless.min.css",
              "projects/weather/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/weather/src/environments/environment.ts",
                  "with": "projects/weather/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "weather:build:production"
            },
            "development": {
              "browserTarget": "weather:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "weather:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/weather/src/test.ts",
            "polyfills": "projects/weather/src/polyfills.ts",
            "tsConfig": "projects/weather/tsconfig.spec.json",
            "karmaConfig": "projects/weather/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/weather/src/favicon.ico",
              "projects/weather/src/assets"
            ],
            "styles": [
              "projects/weather/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "soldado": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/soldado",
      "sourceRoot": "projects/soldado/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/soldado",
            "index": "projects/soldado/src/index.html",
            "main": "projects/soldado/src/main.ts",
            "polyfills": "projects/soldado/src/polyfills.ts",
            "tsConfig": "projects/soldado/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/soldado/src/favicon.ico",
              "projects/soldado/src/assets"
            ],
            "styles": [
              "./node_modules/@picocss/pico/css/pico.classless.min.css",
              "projects/soldado/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/soldado/src/environments/environment.ts",
                  "with": "projects/soldado/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "soldado:build",
            "host": "0.0.0.0"
          },
          "configurations": {
            "production": {
              "browserTarget": "soldado:build:production"
            },
            "development": {
              "browserTarget": "soldado:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "soldado:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/soldado/src/test.ts",
            "polyfills": "projects/soldado/src/polyfills.ts",
            "tsConfig": "projects/soldado/tsconfig.spec.json",
            "karmaConfig": "projects/soldado/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/soldado/src/favicon.ico",
              "projects/soldado/src/assets"
            ],
            "styles": [
              "projects/soldado/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "soldado",
  "cli": {
    "analytics": false
  }
}
